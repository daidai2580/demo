<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.search.mapper.UserMapper">
    <select id="getAllUser" resultType="com.demo.search.pojo.User">
        select * from t_user
    </select>
    <select id="getUserByid" resultType="com.demo.search.pojo.User">
        select * from t_user where id=#{id}
    </select>
</mapper>
 